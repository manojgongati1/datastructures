from collections import deque
class Stack:
    def __init__(self):
        self.container = deque()
        
    def push(self,x):
        self.container.append(x)
        
    def peek(self):
        return self.container[-1]

    def is_empty(self):
        return len(self.container) == 0
    
    def pop(self):
        return self.container.pop()
        
    def size(self):
        return len(self.container)
    
    def __iter__(self):
        return iter(self.container)
    def __iter__(self):
        return iter(self.container)
        
def reverse(self):
        new = Stack()
        
        for c in self:
            new.push(c)
        
        revstr=''
        
        while new.size()!=0:
            revstr+=new.pop()
            
        return revstr
    
    

reverse("LION")
